  object CReceberParc: TSQLQuery
    MaxBlobSize = -1
    Params = <
      item
        DataType = ftDate
        Name = 'D1'
        ParamType = ptInput
      end>
    SQL.Strings = (
      'SELECT Pessoa.Nome, creceberparc.vlrvencimento,'
      'creceberparc.vlrpagto, creceberparc.vlrrestante,'
      'creceberparc.dtvencimento'
      'FROM creceberparc'
      'INNER JOIN pessoa on'
      'Pessoa.id = creceberparc.codcliente'
      'where creceberparc.dtvencimento <= :D1')
    SQLConnection = Dm1.Conexao
    Left = 32
    Top = 65528
  end
