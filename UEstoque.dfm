  object EstoqueMov: TSQLQuery
    MaxBlobSize = -1
    Params = <
      item
        DataType = ftDate
        Name = 'D1'
        ParamType = ptInput
      end>
    SQL.Strings = (
      
        'select estoquemov.codproduto, estoquemov.tamanho, SUM(estoquemov' +
        '.qtd2) QTD, produto.nome'
      'from estoquemov'
      'inner join produto on'
      '(estoquemov.codproduto = produto.id)'
      'where estoquemov.dtmov <= :D1'
      'group by estoquemov.codproduto, estoquemov.tamanho, produto.nome'
      'order by produto.nome, estoquemov.tamanho')
    SQLConnection = Dm1.Conexao
    Left = 216
    Top = 192
  end
